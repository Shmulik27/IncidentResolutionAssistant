[MASTER]
ignore=venv

[TYPECHECK]
ignored-modules=faiss,spacy,sklearn,numpy,scipy,fastapi,pydantic,prometheus_client,sentence_transformers

[FORMAT]
max-line-length=100

[MESSAGES CONTROL]
disable=C0114,C0115,C0116
# C0114: missing-module-docstring
# C0115: missing-class-docstring
# C0116: missing-function-docstring 